<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
  <!-- <link href="/public/css/styles.css"  > -->
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
<div class="container ">
  <div class="flex h-full items-center justify-center w-full">
    <div class="jumbotron w-full flex flex-col items-center">
      
      <h2>User</h2>
    <h3><%= user.id %></h3>
    <h3><%= user.email %></h3>
</div>
  </div>

  <div class="w-full  ">
          <h3>content</h3>
    <div class="w-full flex-col items-center justify-center">

        <ul class="w-full h-full flex flex-wrap items-center justify-center  gap-2">
      <% resp.forEach(function(res) { %>
        <li class="w-[90%] md:w-[30%]   p-2 bg-slate-700 rounded-lg  flex flex-col items-center justify-center p-1">
          <strong><%= res.status %></strong>

          <%= res.data.content %>,
          <%= res.imgurl %>
         <div class="w-full  p-1">
          <!-- <img 
          class="w-full aspect-square rounded-lg"
        src="<%= res.data.imgurl %>" /></div> -->
        </li>
      <% }); %>
    </ul>
    </div>
  </div>
</div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>
